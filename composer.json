{
  "name": "pollen-solutions/wp-kernel",
  "description": "Pollen Wordpress Kernel Component",
  "homepage": "https://github.com/milkcreation/pollen-wp-kernel.git",
  "keywords": [
    "pollen-solutions",
    "component",
    "wordpress",
    "kernel"
  ],
  "time": "2021-09-15",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "pollen-solutions/kernel": "^1.0",
    "pollen-solutions/wp-env": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Pollen\\WpKernel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpKernel\\Test\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
